
function downvote(){

  const likePost = document.querySelector('.like_post');
  const unlikePost = document.querySelector('.unlike_post');
  const likeVoteCount = likePost.querySelector('.vote_count');
  const unlikeVoteCount = unlikePost.querySelector('.vote_count');

  likeVoteCount.innerText = "<%= @post.get_upvotes.size%>"
  unlikeVoteCount.innerText = "<%= @post.get_downvotes.size%>"
}
downvote()
