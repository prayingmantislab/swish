      <% if user_signed_in? %>
<div class="d-flex flex-row navbar navbar-expand-sm navbar-light navbar-lewagon">
  <%= link_to root_path, class: "navbar-brand" do %>
  <div class="d-flex align-items-end">
    <%= cl_image_tag "https://res.cloudinary.com/dg6wajgky/image/upload/v1575541123/basketballlogoblank_js4fxo.png"%>
    <h1 class="logo-text">Swish</h1>
  </div>
    <% end %>

  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>


  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="navbar-nav mr-auto">
        <li class="nav-item active">
          <button class="btn-nav">
          <%= link_to "Home", root_path, class: "nav-link" %>
          </button>
        </li>
        <li>
          <button class="btn-nav">
          <%= link_to "Teams", teams_path, class: "nav-link #{'active-nav-item' if params[:controller] == 'teams'}" %>
          </button>
        </li>
        <li>
          <button class="btn-nav">
          <%= link_to "Forum", posts_path, class: "nav-link #{'active-nav-item' if params[:controller] == 'posts'}" %>
          </button>
        </li>
        <li>
          <button class="btn-nav">
          <%= link_to "Scores", games_path, class: "nav-link" %>
          </button>
        </li>
        <li>
          <button class="btn-nav">
         <%= link_to "Standings", standings_path, class: "nav-link" %>
          </button>
        </li>
        <li>
          <button class="btn-nav">
            <%= link_to "Dashboard", dashboard_path, class: "nav-link" %>
          </button>
        </li>
    </ul>
  </div>

  <% end %>

  <div class="nav-item dropdown btn btn-light">
    <% if user_signed_in? %>
      <% if current_user.photo.file.nil? %>
        <%= cl_image_tag "blank-avatar_rupjxt", class: "avatar dropdown-toggle", id: "navbarDropdown", data: { toggle: "dropdown" }, 'aria-haspopup': true, 'aria-expanded': false %>
      <% else %>
        <%= cl_image_tag current_user.photo, class: "avatar dropdown-toggle", id: "navbarDropdown", data: { toggle: "dropdown" }, 'aria-haspopup': true, 'aria-expanded': false %>
      <% end %>
        <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdown">
          <%= current_user.email %>
          <%= link_to "Dashboard", dashboard_path, class: "dropdown-item" %>
          <%= link_to "Choose Teams", new_follow_path, class: "dropdown-item" %>
          <%= link_to "Choose Players", new_player_follow_path, class: "dropdown-item" %>
          <%= link_to "Log out", destroy_user_session_path, method: :delete, class: "dropdown-item" %>
        </div>
    <% end  %>
  </div>
</div>










